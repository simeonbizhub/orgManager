<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_address"
	title="Address"
	xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
	<form border="true">
		<column responsiveWidth="4" />
		<column />
		<row>
			<item>
				<default binding="levelUnit" />
			</item>
		</row>
		<row>
			<item>
				<default binding="buildingName" />
			</item>
		</row>
		<row>
			<item>
				<default binding="streetAddress" />
			</item>
		</row>
		<row>
			<item>
				<default binding="suburb" />
			</item>
		</row>
		<row>
			<item>
				<default binding="postCode" />
			</item>
		</row>
	</form>
</view>
